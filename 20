#include <iostream>
#include <vector>
using namespace std;

int main() {
    int m, n;
    cin >> m >> n;
    vector<vector<int>> mat(m, vector<int>(n));
    for (int i = 0; i < m; i++)
        for (int j = 0; j < n; j++)
            cin >> mat[i][j];

    int sum = 0;
    long long product = 1;

    for (int i = 0; i < m; i++)
        for (int j = 0; j < n; j++) {
            sum += mat[i][j];
            product *= mat[i][j];
        }

    cout << "Sum: " << sum << endl;
    cout << "Product: " << product << endl;

    return 0;
}
