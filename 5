#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    int N;
    cin >> N;
    vector<vector<int>> mat(N, vector<int>(N));
    vector<int> elements;

    for (int i = 0; i < N; i++)
        for (int j = 0; j < N; j++) {
            cin >> mat[i][j];
            elements.push_back(mat[i][j]);
        }

    sort(elements.begin(), elements.end());

    int k = 0;
    for (int i = 0; i < N; i++)
        for (int j = 0; j < N; j++)
            mat[i][j] = elements[k++];

    for (int i = 0; i < N; i++) {
        for (int j = 0; j < N; j++)
            cout << mat[i][j] << " ";
        cout << endl;
    }

    return 0;
}
