#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    int R, C;
    cin >> R >> C;
    vector<vector<int>> matrix(R, vector<int>(C));
    for (int i = 0; i < R; i++)
        for (int j = 0; j < C; j++)
            cin >> matrix[i][j];

    int low = 1, high = 1e9; 
    int medianPos = (R * C + 1) / 2;

    while (low < high) {
        int mid = low + (high - low) / 2;
        int count = 0;

        for (int i = 0; i < R; i++)
            count += upper_bound(matrix[i].begin(), matrix[i].end(), mid) - matrix[i].begin();

        if (count < medianPos)
            low = mid + 1;
        else
            high = mid;
    }

    cout << low;
    return 0;
}
