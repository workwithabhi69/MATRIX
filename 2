#include <iostream>
#include <vector>
using namespace std;

int main() {
    int m, n, target;
    cin >> m >> n >> target;
    vector<vector<int>> matrix(m, vector<int>(n));
    for (int i = 0; i < m; i++)
        for (int j = 0; j < n; j++)
            cin >> matrix[i][j];

    int left = 0, right = m * n - 1;
    bool found = false;

    while (left <= right) {
        int mid = left + (right - left) / 2;
        int midValue = matrix[mid / n][mid % n];

        if (midValue == target) {
            found = true;
            break;
        } else if (midValue < target)
            left = mid + 1;
        else
            right = mid - 1;
    }

    if (found)
        cout << "True";
    else
        cout << "False";

    return 0;
}
