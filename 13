#include <iostream>
#include <vector>
using namespace std;

int main() {
    int m, n;
    cin >> m >> n;
    vector<vector<int>> mat(m, vector<int>(n));
    for (int i = 0; i < m; i++)
        for (int j = 0; j < n; j++)
            cin >> mat[i][j];

    vector<vector<int>> dp(m, vector<int>(n, 0));
    int maxSize = 0, maxRow = 0, maxCol = 0;

    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            if (i == 0 || j == 0)
                dp[i][j] = mat[i][j];
            else if (mat[i][j] == 1)
                dp[i][j] = 1 + min({dp[i-1][j], dp[i][j-1], dp[i-1][j-1]});
            if (dp[i][j] > maxSize) {
                maxSize = dp[i][j];
                maxRow = i;
                maxCol = j;
            }
        }
    }

    cout << "Largest square sub-matrix of 1's:\n";
    for (int i = maxRow - maxSize + 1; i <= maxRow; i++) {
        for (int j = maxCol - maxSize + 1; j <= maxCol; j++)
            cout << mat[i][j] << " ";
        cout << endl;
    }

    return 0;
}
