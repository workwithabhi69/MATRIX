#include <iostream>
#include <vector>
#include <stack>
using namespace std>

int maxHistogramArea(vector<int> &heights) {
    stack<int> s;
    int maxArea = 0;
    int n = heights.size();
    int i = 0;

    while (i < n) {
        if (s.empty() || heights[i] >= heights[s.top()]) {
            s.push(i++);
        } else {
            int top = s.top(); s.pop();
            int width = s.empty() ? i : i - s.top() - 1;
            maxArea = max(maxArea, heights[top] * width);
        }
    }

    while (!s.empty()) {
        int top = s.top(); s.pop();
        int width = s.empty() ? i : i - s.top() - 1;
        maxArea = max(maxArea, heights[top] * width);
    }

    return maxArea;
}

int main() {
    int n, m;
    cin >> n >> m;
    vector<vector<int>> mat(n, vector<int>(m));
    for (int i = 0; i < n; i++)
        for (int j = 0; j < m; j++)
            cin >> mat[i][j];

    vector<int> heights(m, 0);
    int maxArea = 0;

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if (mat[i][j] == 0)
                heights[j] = 0;
            else
                heights[j] += 1;
        }
        maxArea = max(maxArea, maxHistogramArea(heights));
    }

    cout << maxArea;
    return 0;
}
`
